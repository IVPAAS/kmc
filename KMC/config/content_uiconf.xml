<root>
	<locale>
		<path>locale/{locale}/content_{locale}_ResourceModule.swf</path>
		<language>en_US</language>
	</locale>
	<uiconf>
		<kdp>
			<moderation>@@ModerationUIConfID@@</moderation>
			<drilldown>@@KdpDrilldownForContentUIConfID@@</drilldown>
			<captureThumbnail>@@KdpCaptureThumbnailForContentUIConfID@@</captureThumbnail>
		</kdp>
		<metadata>@@MetadataUIConfID@@</metadata>
		<kclip>
			<ads>@@KClipAdsDrillDownUIConfID@@</ads>
		</kclip>
		<entryDrilldown>
			<!-- mixesTab>false</mixesTab -->
			<panels>
				<panel id="entryMetaData" url="/ked/EntryMetadata.swf" index="0" />
				<panel id="entryThumbnails" url="/ked/EntryThumbnails.swf" index="1" />
				<panel id="entryAcp" url="/ked/EntryAccessControl.swf" index="2" />
				<panel id="entrySch" url="/ked/EntryScheduling.swf" index="3" />
				<panel id="entryAssets" url="/ked/EntryFlavors.swf" index="4" />
				<panel id="contentTab" url="/ked/EntryContent.swf" index="5" />
				<panel id="liveStream" url="/ked/EntryLiveStreaming.swf" index="6" />
				<panel id="customData" url="/ked/EntryCustomData.swf" index="7" />
				<panel id="entryDistribution" url="/ked/EntryDistribution.swf" index="8" />
				<panel id="entryCaptions" url="/ked/EntryCaptions2.swf" index="9" />
				<panel id="ads" url="/ked/EntryAds.swf" index="10" />
				<panel id="entryRelated" url="/ked/EntryRelated.swf" index="11" />
				<panel id="entryClips" url="/ked/EntryClips.swf" index="12" />
			</panels>
		</entryDrilldown>
	</uiconf>
	<!-- bulkupload sample files -->
	<bulkupload>/content/docs/kaltura_batch_upload_eagle.zip</bulkupload>
	<!-- cueopints sample -->
	<cuepoints>/content/docs/xml/adCuePointSampleFile.xml</cuepoints>
</root>