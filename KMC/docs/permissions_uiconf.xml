<root>
	<permissions>
		<!-- Content Ingestion -->
		<permissionGroup text="Content Ingestion" id="CONTENT_INGEST_BASE">
			<permission text="KCW" id="CONTENT_INGEST_UPLOAD">
				<ui id="content.upload.profilesCb" enabled="false"/>
				<ui id="content.upload.kcwBtn" enabled="false" buttonMode="false"/>
			</permission>
			<permission text="Bulk Upload" id="CONTENT_INGEST_BULK_UPLOAD">
				<ui id="content.upload.submitBtn" enabled="false"/>
			</permission>
			<permission text="Create Remix" id="CONTENT_INGEST_ADD_MIX"></permission>
		</permissionGroup>
		<!-- Content Management -->
		<permissionGroup text="Content Management" id="CONTENT_MANAGE_BASE">
			<permission text="Modify Metadata" id="CONTENT_MANAGE_METADATA">
				<ui id="entryDrilldown.entryMetaData.name_input" enabled="false"/>
				<ui id="entryDrilldown.entryMetaData.descriptionTi" enabled="false"/>
				<ui id="entryDrilldown.entryMetaData.tagsTi" enabled="false"/>
			</permission>
			<permission text="Modify Categories" id="CONTENT_MANAGE_ASSIGN_CATEGORIES">
				<ui id="entryDrilldown.entryMetaData.categoriesTextInput" enabled="false"/>
				<ui id="content.manage.entryTable" dragEnabled="false" dragMoveEnabled="false" dropEnabled="false"/>
			</permission>
			<permission text="Modify Thumbnail" id="CONTENT_MANAGE_THUMBNAIL">
				<ui id="entryDrilldown.entryMetaData.thumbnailOptionBox" visible="false"/>
			</permission>
			<permission text="Modify Schedule" id="CONTENT_MANAGE_SCHEDULE">
				<ui id="entryDrilldown" hideTabs="entrySch"/>
			</permission>
			<permission text="Modify Access Control" id="CONTENT_MANAGE_ACCESS_CONTROL">
				<ui id="entryDrilldown.entryAcp.accessControlProfliesComboBox" enabled="false"/>
				<ui id="entryDrilldown.entryAcp.addBtn" visible="false"/>
			</permission>
			<permission text="Manage Custom Data" id="CONTENT_MANAGE_CUSTOM_DATA">
				<ui id="entryDrilldown.customData" enabled="false"/>
			</permission>
			<permission text="Delete Content" id="CONTENT_MANAGE_DELETE">
				<ui id="content.manage.toolBar.deleteBtn" enabled="false" includeInLayout="false"/>
			</permission>
			<permission text="grab embed code" id="CONTENT_MANAGE_EMBED_CODE"></permission>
			<permission text="Modify Flavors" id="CONTENT_MANAGE_RECONVERT">
				<ui id="entryDrilldown.entryAsstes" hideColumns="8"/>
			</permission>
			<permission text="manage live stream" id="CONTENT_MANAGE_ADD_LIVE">
				<ui id="content.manage.toolBar.newStreamBtn" visible="false" includeInLayout="false"/>
			</permission>
			<permission text="edit categories" id="CONTENT_MANAGE_EDIT_CATEGORIES">
				<ui id="content.manage.filter" showAccordionHeaderButtons="false" />
			</permission>
		</permissionGroup>
		<!-- Content Moderation -->
		<permissionGroup text="Content Moderation" id="CONTENT_MODERATE_BASE">
			<permission text="Moderate metadata" id="CONTENT_MODERATE_METADATA">
				<ui id="entryDrilldown.entryMetaData.name_input" editable="false"/>
				<ui id="entryDrilldown.entryMetaData.descriptionTi" editable="false"/>
				<ui id="entryDrilldown.entryMetaData.tagsTi" editable="false"/>
			</permission>
		</permissionGroup>
		<!-- Playlist management -->
		<permissionGroup text="Playlist management" id="PLAYLIST_BASE">
			<permission text="create playlist" id="PLAYLIST_ADD">
				<ui id="content.playlists.actionsBox" visible="false" includeInLayout="false" />
			</permission>
			<permission text="modify playlist" id="PLAYLIST_UPDATE">
				<ui id="ruleBasedPlaylistDrilldown" editable="false" />
				<ui id="manualPlaylistDrilldown" editable="false" />
			</permission>
			<permission text="delete playlist" id="PLAYLIST_DELETE">
				<ui id="content.playlists.toolBar.deleteBtn" visible="false" />
			</permission>
			<permission text="grab playlist embed code" id="PLAYLIST_EMBED_CODE"></permission>
		</permissionGroup>
		<!-- Syndication management -->
		<permissionGroup text="Syndication management" id="SYNDICATION_BASE">
			<permission text="create syndication feeds" id="SYNDICATION_ADD">
				<ui id="content.syndication.addBtn" enabled="false" buttonMode="false"/>
			</permission>
			<permission text="modify syndication feeds" id="SYNDICATION_UPDATE">
				<ui id="externalSyndicationDrilldown" editable="false"/>
			</permission>
			<permission text="delete syndication feeds" id="SYNDICATION_DELETE">
				<ui id="content.syndication.deleteExSynBtn" visible="false" />
			</permission>
		</permissionGroup>
		<!-- Studio -->
		<permissionGroup text="Studio" id="STUDIO_BASE">
			<permission text="create player" id="STUDIO_ADD_UICONF">
				<ui id="studio.playerList" comboHideItems="duplicate"/>
				<ui id="studio.playerList.templatesHolder" visible="false" includeInLayout="false"/>
			</permission>
			<permission text="modify players" id="STUDIO_UPDATE_UICONF">
				<ui id="wizard.stylePage.all" enabled="false" />
				<ui id="wizard.advertizingPage.all" enabled="false" />
				<ui id="wizard.templatePage.all" enabled="false" />
				<ui id="wizard.previewPlayerPage.controls" enabled="false" />
				<ui id="wizard.contentPage.removeBtn" enabled="false" />
				<ui id="wizard.contentPage.addBtn" enabled="false" buttonMode="false"/>
				<ui id="wizard.contentPage.upBtn" visible="false" />
				<ui id="wizard.contentPage.downBtn" visible="false" />
				<ui id="wizard.featurePage" editable="false" />
				<ui id="wizard.wizardSaveBtn" visible="false" />
			</permission>
			<permission text="delete players" id="STUDIO_DELETE_UICONF">
				<ui id="studio.playerList" comboHideItems="delete"/>
			</permission>
			<permission text="Set advertising settings" id="ADVERTISING_UPDATE_SETTINGS">
				<ui id="wizard" hideTabs="advertizingPage"/>
			</permission>
		</permissionGroup>
		
		<!-- Account settings -->
		<permissionGroup text="Account settings" id="ACCOUNT_BASE">
			<permission text="account" id="ACCOUNT_UPDATE_SETTINGS">
				<ui id="account.overview.infoBox" enabled="false" />
				<ui id="account.overview.saveChangesBtn" visible="false" />
				<ui id="account.overview.saveBtn" visible="false" />
				<ui id="account.overview.loginBox" enabled="false" />
			</permission>
		</permissionGroup>
		<!-- Integration settings -->
		<permissionGroup text="Integration settings" id="INTEGRATION_BASE">
			<permission text="integration" id="INTEGRATION_UPDATE_SETTINGS">
				<ui id="account.integration.advCont" enabled="false" />
				<ui id="account.integration.notifications" enabled="false" />
				<ui id="account.integration.notificationsTable" enabled="false" />
				<ui id="account.integration.saveChangesBtn" visible="false" />
			</permission>
		</permissionGroup>
		<!-- Access control settings -->
		<permissionGroup text="Access control settings" id="ACCESS_CONTROL_BASE">
			<permission text="Create" id="ACCESS_CONTROL_ADD"></permission>
			<permission text="modify" id="ACCESS_CONTROL_UPDATE"></permission>
			<permission text="delete" id="ACCESS_CONTROL_DELETE"></permission>
		</permissionGroup>
		<!-- Transcoding settings -->
		<permissionGroup text="Transcoding settings" id="TRANSCODING_BASE">
			<permission text="Create" id="TRANSCODING_ADD"></permission>
			<permission text="modify" id="TRANSCODING_UPDATE"></permission>
			<permission text="delete" id="TRANSCODING_DELETE"></permission>
		</permissionGroup>
		<!-- Custom data settings -->
		<permissionGroup text="Custom data settings" id="CUSTOM_DATA_PROFILE_BASE">
			<permission text="Create" id="CUSTOM_DATA_PROFILE_ADD"></permission>
			<permission text="modify" id="CUSTOM_DATA_PROFILE_UPDATE"></permission>
			<permission text="delete" id="CUSTOM_DATA_PROFILE_DELETE"></permission>
		</permissionGroup>
		<!-- Administration -->
		<permissionGroup text="Administration" id="ADMIN_BASE">
			<permission text="Create users" id="ADMIN _USER_ADD"></permission>
			<permission text="modify users" id="ADMIN _USER_UPDATE">
				<ui id="admin.users.table" comboHideItems="edit"/>
			</permission>
			<permission text="delete users" id="ADMIN _USER_DELETE"></permission>
			<permission text="Create roles" id="ADMIN _ROLE_ADD"></permission>
			<permission text="modify roles" id="ADMIN _ROLE_UPDATE"></permission>
			<permission text="delete roles" id="ADMIN _ROLE_DELETE"></permission>
		</permissionGroup>
		<!-- Video Analytics -->
		<permissionGroup text="Video Analytics" id="ANALYTICS_BASE">
		</permissionGroup>
		<!-- -->
	</permissions>
	
	<!-- -->
	<!-- -->
	<!-- -->
	
	<uimapping>
		<module id="content">
			<tab id="upload">
				<permission id="CONTENT_INGEST_UPLOAD"/>
				<permission id="CONTENT_INGEST_BULK_UPLOAD"/>
			</tab>
			<tab id="syndication">
				<permission id="SYNDICATION_ADD"/>
				<permission id="SYNDICATION_UPDATE"/>
				<permission id="SYNDICATION_DELETE"/>
			</tab>
		</module>
		<module id="analytics">
			<permission id="44"/>
		</module>
		<!-- this module requres 4 of 5 boxes -->
		<module id="dashboard" min="4">
			<!-- embed player -->
			<permission id="13"/>
			<!-- studio permission -->
			<permission id="23"/>
			<!-- analytics permission -->
			<permission id="44"/>
			<!-- upload permission -->
			<permission id="2"/>
			<!-- account and billing permission -->
			<permission id="32"/>
		</module>
	</uimapping>
</root>